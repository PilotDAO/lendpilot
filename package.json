{
  "name": "aavescan-clone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run --dir tests/integration",
    "test:e2e": "playwright test",
    "preflight": "tsx scripts/preflight.ts",
    "scrape-aavescan": "tsx scripts/scrape-aavescan.ts",
    "fetch-markets": "tsx scripts/fetch-all-markets.ts",
    "extract-schemas": "tsx scripts/extract-schemas.ts",
    "codegen": "graphql-codegen --config codegen-aavekit.yml && graphql-codegen --config codegen-subgraph.yml",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "sync:market-data": "tsx scripts/sync-market-data.ts"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@tanstack/react-table": "^8.20.0",
    "big.js": "^6.2.1",
    "echarts": "^5.5.0",
    "echarts-for-react": "^3.0.2",
    "graphql": "^16.9.0",
    "graphql-request": "^6.1.0",
    "lru-cache": "^11.0.0",
    "next": "^14.2.0",
    "prisma": "^7.2.0",
    "prisma-adapter-postgres": "^1.0.2",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-graphql-request": "^6.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@playwright/test": "^1.44.0",
    "@types/big.js": "^6.2.0",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.0",
    "prettier": "^3.3.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.5.0",
    "vitest": "^1.6.0"
  }
}
